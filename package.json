{
  "name": "aufbau",
  "productName": "Aufbau",
  "version": "0.1.0",
  "description": "A desktop app to view and use a hand-curated, artisanal selection of local web apps",
  "license": "MIT",
  "main": "index.js",
  "repository": "mhkeller/aufbau",
  "author": {
    "name": "mhkeller",
    "email": "michael.keller@gmail.com",
    "url": "github.com/mhkeller/aufbau"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "start": "electron .",
    "build-skip-install": "electron-packager . Aufbau --out=aufbau-dist --ignore=aufbau-dist --asar --platform=darwin --arch=x64 --version=0.30.1 --overwrite --icon=assets/aufbau.icns",
    "build": "npm run build-skip-install",
    "install-apps": "./install-apps.js",
    "postinstall": "npm run install-apps",
    "prebuild": "npm run install-apps"
  },
  "keywords": [
    "electron-app"
  ],
  "dependencies": {
    "chalk": "^1.1.0",
    "cheerio": "^0.19.0",
    "electron-debug": "^0.1.1",
    "indian-ocean": "^0.4.0",
    "queue-async": "^1.0.7",
    "shelljs": "^0.5.1"
  },
  "devDependencies": {
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.30.0"
  }
}
